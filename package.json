{
  "name": "infinity-todo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint \"*/**/*.{tsx,ts,jsx,js,json}\" --fix",
    "formatter": "prettier --list-different --write",
    "format:root": "yarn formatter \"*.{tsx,ts,jsx,js,json}\"",
    "format:app": "yarn formatter \"src/**/*.{tsx,ts,jsx,js,json}\"",
    "format": "yarn format:root && yarn format:app",
    "test-runner": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand --passWithNoTests",
    "test": "yarn test-runner",
    "test:watch": "yarn test-runner --watch",
    "test:cov": "yarn test-runner --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "draft-js": "^0.11.7",
    "html5-boilerplate": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/draft-js": "^0.11.9",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-react": "^1.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "^2.8.0",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
